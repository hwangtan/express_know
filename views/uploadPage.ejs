<div class="panel panel-default">
  <div class="panel-heading"><h1>Images</h1></div>
  <div class="panel-body well well-lg">
    <form method='post' action='/upload' class='form-horizontal' enctype="multipart/form-data">
    <div class='form-group'>
        <label for='img_file'>Select Image</label>
        <input type='file'
               class='form-control'
               name='img_file'
               id='img_file' />
    </div>
    <div class='form-group'>
      <button type='submit' class='btn btn-warning'>UPLOAD</button>
    </div>
  </form>
  </div>
  <% for(var i = 0; i < imgArray.length; i++) { %>
  <img   src="/images/<%- imgArray[i] %>"
       class="img-responsive img-thumbnail"
         alt="Responsive image"
       width="250"
      height="250" />
  <% } %>
</div>
